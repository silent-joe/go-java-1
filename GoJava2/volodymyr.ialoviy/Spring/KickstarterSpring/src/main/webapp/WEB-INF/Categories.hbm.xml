<?xml version="1.0" encoding="UTF-8"?>
<!DOCTYPE hibernate-mapping PUBLIC "-//Hibernate/Hibernate Mapping DTD 3.0//EN" "http://hibernate.sourceforge.net/hibernate-mapping-3.0.dtd" >
<hibernate-mapping>

		<class name="ua.com.goit.gojava2.vova.kickstarter.model.Category" table="Categories">
			<id column="id_category" name="idCategory" type="java.lang.Long">
				<generator class="increment" />
			</id>
			<property column="name_category" name="nameCategory" type="java.lang.String" />

		</class>

</hibernate-mapping>